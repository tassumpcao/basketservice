  },
  "homepage": "https://github.com/tassumpcao/basketservice#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "consign": "^0.1.6",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.4.0",
    "errors": "^0.3.0",
    "express": "^4.17.1",
    "pino": "^5.13.6"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
